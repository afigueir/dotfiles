* {
	background-color: #000000;
	border-color: #ffffff;
	text-color: #808080;
}

#window {
	archor: north;
	location: north;
	width: 100%;
	padding: 5px;
	children: [ horibox ];
}

#horibox {
	orientation: horizontal;
	children: [ prompt, entry, listview ];
}

#listview {
	layout: horizontal;
	spacing: 5px;
	lines: 100;
}

#entry {
	expand: false;
	width: 10em;
	padding: 0 10px;
}

#element {
	padding: 0px 2px;
}

#element selected {
	text-color: #ffffff;
}
